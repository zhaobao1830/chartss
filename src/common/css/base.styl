.LDpage .mengC .tableList .loading{
  left: 0px;
  top: 0px;
  width: 1200px;
  height: 450px;
  text-align: center;
  line-height: 450px;
  z-index: 99;
  position: absolute;
  background-color: white;
}
.LDpage .mengC .page{
  overflow: hidden;
}
.LDpage .mengC .M-box{
  margin-top: 20px;
  float: right;
}
.LDpage .mengC tbody{
  color: #2e2f32;
}
.LDpage .close{
  width: 40px;
  height: 48px;
  background: url(http://static.crecgec.com/leader/close.png) center center no-repeat;
  float: right;
}
.meng .classChioce{
  margin-bottom: 10px;
  overflow: hidden;
}
.meng .classChioce a{
  width:80px;
  height: 30px;
  line-height: 30px;
  font-size: 16px;
  background-color: #777;
  border-radius: 5px;
  text-align: center;
  float: left;
  margin-right: 10px;
  color: white;
  cursor: pointer;
}
.meng .classChioce a.daochu{
  margin-right: 18px;
  float: right;
}
.meng .classChioce a.select{
  background-color: #0169b7;
}
[v-cloak] {
  display: none;
}
